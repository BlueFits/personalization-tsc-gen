{
  "name": "@bluefits/pers-tsc-gen",
  "version": "1.0.2",
  "description": "",
  "main": "index.js",
  "bin": "./bin/cli.js",
  "scripts": {
    "build": "npm run clean && webpack --config webpack.config.prod.js",
    "build-dev": "webpack",
    "start": "webpack && concurrently \"webpack --watch\" \"npm run start-dev\"",
    "serve": "webpack serve",
    "clean": "rimraf dist/*",
    "cleanlivereload": "rimraf services/LiveReload/dist/*",
    "compile-reload": "npm run cleanlivereload && tsc -b services/LiveReload",
    "start-dev": "npm run compile-reload && node ./services/LiveReload/dist/index.js",
    "devstart": "cd ./services/LiveReload/ && webpack"
  },
  "author": "Christian Rojas",
  "license": "ISC",
  "devDependencies": {
    "@types/jquery": "^3.5.14",
    "@types/node": "14.x",
    "@types/puppeteer": "^5.4.6",
    "cheerio": "^1.0.0-rc.12",
    "chokidar": "^3.5.3",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.4.0",
    "ejs-loader": "^0.5.0",
    "html-webpack-plugin": "^5.5.0",
    "inline-source-webpack-plugin": "^2.0.1",
    "log-timestamp": "^0.3.0",
    "puppeteer": "^13.7.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  }
}
